{
  "name": "nekomaid-ui-lang",
  "displayName": "NekoMaid UI Lang",
  "description": "Adds syntax highlighting support for `.neko_ui` files.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "nekomaid",
      "aliases": ["NekoMaid UI", "nekomaid"],
      "extensions": [".neko_ui"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "nekomaid",
      "scopeName": "source.neko_ui",
      "path": "./syntaxes/nekomaid.tmLanguage.json"
    }]
  }
}
